{
    "common": {
        "name":                     "dwd",
        "title":                    "DWD",
        "desc":                     "Wetterwarnungen des deutschen Wetterdienstes",
        "version": "0.1.2",
        "mode":                     "schedule",
        "schedule":                 "0 * * * *",
        "platform":                 "javascript/Node.js",
        "loglevel":                 "info",
        "keywords":                 ["weather", "Wetter", "warning", "Warnung", "forecast", "Vorhersage", "rain", "Regen", "wind"],
        "main":                     "dwd.js",
        "authors": [
            "hobbyquaker <hq@ccu.io>"
        ],
        "license":                  "MIT",
        "readme":                   "https://github.com/ioBroker/ioBroker.dwd/blob/master/README.md",
        "icon":                     "dwd.png",
        "extIcon":                  "https://raw.githubusercontent.com/ioBroker/ioBroker.dwd/master/admin/dwd.png",
        "localLink":                "http://www.dwd.de/"
    },
    "native": {
        "host": "ftp-outgoing2.dwd.de",
        "user": "gds12345",
        "pass": "xxxx",
        "dienststelle": "SU",
        "kreis": "S"
    },
    "objects": [


    ],
    "instanceObjects": [
        {
            "_id":  "warning",
            "type": "channel",
            "role": "forecast",
            "common": {
                "name": ""
            },
            "native": {}
        },
        {
            "_id":  "warning.begin",
            "type": "state",
            "common": {
                "name":  "Warning begin",
                "type":  "datetime",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "warning.end",
            "type": "state",
            "common": {
                "name":  "Warning end",
                "type":  "datetime",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "warning.severity",
            "type": "state",
            "common": {
                "name":  "Warning severity",
                "type":  "number",
                "read":  true,
                "write": false,
                "states": {
                    "0": "None",
                    "1": "Minor",
                    "2": "Moderate",
                    "3": "Severe",
                    "4": "Extreme"
                }
            },
            "native": {}
        },
        {
            "_id":  "warning.text",
            "type": "state",
            "common": {
                "name":  "Warning text",
                "type":  "string",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.headline",
            "type": "state",
            "common": {
                "name":  "Warning headline",
                "type":  "string",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.description",
            "type": "state",
            "common": {
                "name":  "Warning description",
                "type":  "string",
                "read":  true,
                "write": false
            },
            "native": {}
        }
    ]
}