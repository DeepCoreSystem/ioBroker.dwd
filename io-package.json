{
    "common": {
        "name":                     "dwd",
        "title":                    "DWD",
        "desc":                     "Wetterwarnungen des deutschen Wetterdienstes",
        "version": "2.0.0",
        "mode":                     "schedule",
        "schedule":                 "3 * * * *",
        "platform":                 "javascript/Node.js",
        "loglevel":                 "info",
        "keywords":                 ["weather", "Wetter", "warning", "Warnung", "forecast", "Vorhersage", "rain", "Regen", "wind"],
        "main":                     "dwd.js",
        "authors": [
            "bluefox <dogafox@gmail.com>"
        ],
        "license":                  "MIT",
        "readme":                   "https://github.com/ioBroker/ioBroker.dwd/blob/master/README.md",
        "icon":                     "dwd.png",
        "extIcon":                  "https://raw.githubusercontent.com/ioBroker/ioBroker.dwd/master/admin/dwd.png",
        "localLink":                "http://www.dwd.de/",
		"allowInit":                true,
        "type":                     "weather"
    },
    "native": {
        "url":      "http://www.dwd.de/DWD/warnungen/warnapp/json/warnings.json",
        "region":   "Kreis Calw",
        "warnings": 2
    },
    "objects": [


    ],
    "instanceObjects": [
        {
            "_id":  "warning",
            "type": "channel",
            "common": {
                "role": "forecast",
                "name": ""
            },
            "native": {}
        },
        {
            "_id":  "warning.begin",
            "type": "state",
            "common": {
                "name":  "Warning begin",
                "type":  "datetime",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "warning.end",
            "type": "state",
            "common": {
                "name":  "Warning end",
                "type":  "datetime",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id": "warning.severity",
            "type": "state",
            "common": {
                "name":  "Warning severity",
                "type":  "number",
                "read":  true,
                "write": false,
                "states": {
                    "0": "None",
                    "1": "Minor",
                    "2": "Moderate",
                    "3": "Severe",
                    "4": "Extreme"
                }
            },
            "native": {}
        },
        {
            "_id":  "warning.text",
            "type": "state",
            "common": {
                "name":  "Warning text",
                "type":  "string",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.headline",
            "type": "state",
            "common": {
                "name":  "Warning headline",
                "type":  "string",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.description",
            "type": "state",
            "common": {
                "name":  "Warning description",
                "type":  "string",
                "read":  true,
                "write": false
            },
            "native": {}
        },
        {
            "_id":  "warning.object",
            "type": "state",
            "common": {
                "name":  "JSON object with warning",
                "type":  "object",
                "read":  true,
                "write": false
            },
            "native": {}
        }
    ]
}